	keywords = {High-Throughput Nucleotide Sequencing, Chromosome Mapping, Genome, Human, Humans, Genomics, 1000 Genomes, Alleles, Gene Frequency, Genetics, Population, Genotype, HLA, HLA Antigens, mapping bias, NGS}
}

