@article{buhler_hla_2011,
	title = {{HLA} {DNA} sequence variation among human populations: molecular signatures of demographic and selective events},
	issn = {1932-6203},
	shorttitle = {{HLA} {DNA} sequence variation among human populations},
	doiNOTSHOWN = {10.1371/journal.pone.0014643},
	abstract = {Molecular differences between HLA alleles vary up to 57 nucleotides within the peptide binding coding region of human Major Histocompatibility Complex (MHC) genes, but it is still unclear whether this variation results from a stochastic process or from selective constraints related to functional differences among HLA molecules. Although HLA alleles are generally treated as equidistant molecular units in population genetic studies, DNA sequence diversity among populations is also crucial to interpret the observed HLA polymorphism. In this study, we used a large dataset of 2,062 DNA sequences defined for the different HLA alleles to analyze nucleotide diversity of seven HLA genes in 23,500 individuals of about 200 populations spread worldwide. We first analyzed the HLA molecular structure and diversity of these populations in relation to geographic variation and we further investigated possible departures from selective neutrality through Tajima's tests and mismatch distributions. All results were compared to those obtained by classical approaches applied to HLA allele frequencies.Our study shows that the global patterns of HLA nucleotide diversity among populations are significantly correlated to geography, although in some specific cases the molecular information reveals unexpected genetic relationships. At all loci except HLA-DPB1, populations have accumulated a high proportion of very divergent alleles, suggesting an advantage of heterozygotes expressing molecularly distant HLA molecules (asymmetric overdominant selection model). However, both different intensities of selection and unequal levels of gene conversion may explain the heterogeneous mismatch distributions observed among the loci. Also, distinctive patterns of sequence divergence observed at the HLA-DPB1 locus suggest current neutrality but old selective pressures on this gene. We conclude that HLA DNA sequences advantageously complement HLA allele frequencies as a source of data used to explore the genetic history of human populations, and that their analysis allows a more thorough investigation of human MHC molecular evolution.},
	language = {eng},
	journal = {PloS One},
	author = {Buhler, St\'ephane and Sanchez-Mazas, Alicia},
	monthNOTSHOWN = feb,
	year = {2011},
	pmid = {21408106},
	pmcid = {PMC3051395},
	keywords = {DNA, Base Sequence, Humans, Genetic Variation, Gene Frequency, Genetics, Population, HLA Antigens, African Continental Ancestry Group, Asian Continental Ancestry Group, European Continental Ancestry Group, HLA-DP beta-Chains, Polymorphism, Genetic}
}

